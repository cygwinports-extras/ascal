  /usr/share/ascal/pixmaps/board.png
  /usr/share/ascal/pixmaps/board_bg.png
  /usr/share/ascal/pixmaps/highlField.png
  /usr/share/ascal/pixmaps/player1.png
  /usr/share/ascal/pixmaps/player1hl.png
  /usr/share/ascal/pixmaps/player2.png
  /usr/share/ascal/pixmaps/player2hl.png
  /usr/share/doc/Cygwin/ascal-0.1.1.README
  /usr/share/doc/ascal-0.1.1/AUTHORS
  /usr/share/doc/ascal-0.1.1/COPYING
  /usr/share/doc/ascal-0.1.1/ChangeLog
  /usr/share/doc/ascal-0.1.1/INSTALL
  /usr/share/doc/ascal-0.1.1/NEWS
  /usr/share/doc/ascal-0.1.1/README
  /usr/share/doc/ascal-0.1.1/THANKS
