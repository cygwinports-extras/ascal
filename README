  /usr/share/ascal/ascal.glade
  /usr/share/ascal/pixmaps/ascal.png
  /usr/share/ascal/pixmaps/board.png
  /usr/share/ascal/pixmaps/board_bg.png
  /usr/share/ascal/pixmaps/highlField.png
