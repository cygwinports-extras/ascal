  /usr/share/ascal/pixmaps/player1hl.png
  /usr/share/ascal/pixmaps/player2.png
  /usr/share/ascal/pixmaps/player2hl.png
  /usr/share/doc/Cygwin/ascal-0.1.0a.README
  /usr/share/doc/ascal-0.1.0a/AUTHORS
  /usr/share/doc/ascal-0.1.0a/COPYING
  /usr/share/doc/ascal-0.1.0a/ChangeLog
  /usr/share/doc/ascal-0.1.0a/INSTALL
